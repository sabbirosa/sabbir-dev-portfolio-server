{
  "name": "sabbir-dev-portfolio-v2-server",
  "version": "1.0.0",
  "description": "Backend server for Sabbir Ahmed Portfolio v2 - Express.js API with JWT authentication",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "seed": "tsx src/scripts/seed.ts",
    "vercel-build": "tsc && tsc-alias"
  },
  "keywords": [
    "express",
    "typescript",
    "jwt",
    "portfolio",
    "api"
  ],
  "author": "Sabbir Ahmed",
  "license": "MIT",
  "dependencies": {
    "@types/mongoose": "^5.11.96",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  }
}
